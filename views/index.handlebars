  <main>
    <div class="row">
      {{#each port}}
        <div class="col s6">
          <div class="card z-depth-0">
              <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src={{ image }} height='230' width='240'>
              </div>
              <div class="card-content">
                <span class="card-title activator grey-text text-darken-4">{{ title }}</span>
              </div>
              <div class="card-reveal">
                <span class="card-title grey-text text-darken-4"> {{ title }} </span>
                <p>{{ description }}</p>
                <p><a class="link" href="{{ link }}">Github Repo</a></p>
              </div>
            </div>
          </div>
        {{/each}}
    </div>
      <div class="row">
        <hr />
        <h5>
          Skills, Languages, and Frameworks
        </h5>
        <ul id="skills-list">
