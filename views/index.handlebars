
  <header>
    <nav class="z-depth-0">
      <div class="nav-wrapper white">
        <a href="/" class="brand-logo left black-text">Danny Walden</a>
        <ul id="nav-mobile" class="right">
          <li><a href="#contact">Contact</a></li>
        </ul>
      </div>
    </nav>
  </header>

  <main>
    <div class="row">
      {{#each port}}
        <div class="col s12 m6">
          <div class="card z-depth-0">
              <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src={{ image }} height='230' width='240'>
              </div>
              <div class="card-content">
                <span class="card-title activator grey-text text-darken-4">{{ title }}</span>
              </div>
              <div class="card-reveal">
                <span class="card-title grey-text text-darken-4"> {{ title }} <i class="material-icons right">close</i></span>
                <p>{{ description }}</p>
                <p><a class="link" href="{{ link }}">Link</a></p>
              </div>
            </div>
          </div>
        {{/each}}
    </div>
  </main>


